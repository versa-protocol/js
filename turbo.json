{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "build/**", "dist/**"],
      "env": []
    },
    "test": {
      "dependsOn": [],
      "outputs": [],
      "inputs": ["src/**/*.test.tsx", "src/**/*.test.ts"],
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
